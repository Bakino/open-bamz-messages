{
  "routing": "HASH",
  "viewsPath": "views",
  "routes": [
    {
      "url": "/",
      "path": "root",
      "routes": [
        {
          "url": "/settings",
          "path": "settings"
        },
        {
          "url": "/welcome",
          "path": "welcome",
          "defaultChild": true
        },
        {
          "url": "/messages",
          "path": "messages"
        },
        {
          "url": "/help",
          "path": "help"
        },
        {
          "url": "/templates",
          "path": "templates"
        }
      ]
    },
     {
      "url": "/popup-smtp{/:code}",
      "path": "views/popup-smtp"
    },
    {
      "url": "/popup-message-detail/:id",
      "path": "views/popup-message-detail"
    },
    {
      "url": "/popup-send-message",
      "path": "views/popup-send-message"
    },
    {
      "url": "/popup-template{/:code}",
      "path": "views/popup-template"
    },
    {
      "url": "/popup-template-send-mail/:template",
      "path": "views/popup-template-send-mail"
    }
  ],
  "extensions": [
    { "url": "https://cdn.jsdelivr.net/gh/Bakino/open-bamz-database@68c8323a62db4aa1253000e8033aa90dc8d1ce5e/front/lib/viewz-dbadmin.mjs" },
    { "url": "https://cdn.jsdelivr.net/gh/Bakino/open-bamz-bootstrap@latest/front/lib/viewz-bootstrap5.mjs" },
    { "url": "/plugin/open-bamz-users/lib/viewz-users.mjs" }
  ]
} 